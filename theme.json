{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "no-title",
			"title": "No Title Template",
			"postTypes": ["page", "post"]
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"slug": "foreground",
					"color": "#242321",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#FCFBF8",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#242321",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#666666",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#CFCFCF",
					"name": "Tertiary"
				},
				{
					"slug": "caramel-brown",
					"color": "#E5D9D1",
					"name": "Caramel Brown"
				},
				{
					"slug": "chocolate-brown",
					"color": "#573B28",
					"name": "Chocolate Brown"
				},
				{
					"slug": "froth-white",
					"color": "#EEEBE9",
					"name": "Froth White"
				},
				{
					"slug": "caramel-brown-light",
					"color": "#E0D9D5",
					"name": "Chocolate Brown(Light)"
				},
				{
					"slug": "cappuccino-brown",
					"color": "#BA691C",
					"name": "Cappuccino Brown"
				},
				{
					"slug": "espresso-black",
					"color": "#000000",
					"name": "Espresso Black"
				}
			]
		},
		"custom": {
			"spacing": {
				"small": "clamp(20px, 4vw, 40px)",
				"medium": "clamp(48px, 8vw, 100px)",
				"large": "clamp(100px, 12vw, 460px)",
				"outer": "min(4vw, 90px)"
			},
			"radius": {
				"rounded-rectangle": "10px",
				"rounded-biscuit": "10%",
				"circle": "50%"
			}
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": 700,
							"src": ["file:./assets/src/font/Poppins/Poppins-Bold.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": 600,
							"src": ["file:./assets/src/font/Poppins/Poppins-SemiBold.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": 500,
							"src": ["file:./assets/src/font/Poppins/Poppins-Medium.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": 400,
							"src": ["file:./assets/src/font/Poppins/Poppins-Regular.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": 300,
							"src": ["file:./assets/src/font/Poppins/Poppins-Light.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": 200,
							"src": ["file:./assets/src/font/Poppins/Poppins-ExtraLight.woff2"]
						},
						{
							"fontFamily": "Poppins",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": 100,
							"src": ["file:./assets/src/font/Poppins/Poppins-Thin.woff2"]
						}
					],
					"fontFamily": "\"Poppins\", sans-serif",
					"name": "Poppins",
					"slug": "poppins"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.125rem",
						"max": "1.75rem"
					},
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "2.25rem"
					},
					"size": "2.25rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "3rem",
						"max": "4.5rem"
					},
					"size": "4.5rem",
					"slug": "xx-large"
				}
			]
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1260px"
		},
		"spacing": {
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/heading": {
				"typography": {
					"fontWeight": "100",
					"lineHeight": "1.1"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-comments": {
				"elements": {
					"h3": {
						"typography": {
							"textTransform": "uppercase"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/post-navigation-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "100",
					"textTransform": "uppercase"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/pullquote": {
				"border": {
					"width": "0"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--medium)",
						"bottom": "var(--wp--custom--spacing--medium)"
					}
				},
				"typography": {
					"fontWeight": "100",
					"lineHeight": "1.2"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--foreground)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "400",
					"textTransform": "uppercase"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--caramel-brown-light)",
			"text": "var(--wp--preset--color--chocolate-brown)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"button": {
				"border": {
					"radius": "var(--wp--custom--radius--rounded-rectangle)"
				},
				"color": {
					"background": "var(--wp--preset--color--chocolate-brown)",
					"text": "var(--wp--preset--color--froth-white)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--typography--font-size--normal)",
					"textTransform": "uppercase"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--cappuccino-brown)",
						"text": "var(--wp--preset--color--froth-white)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--espresso-black)",
						"text": "var(--wp--preset--color--froth-white)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--background)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--background)"
					}
				}
			},
			"link": {
				"typography": {
					"textDecoration": "none"
				},
				"color": {
					"text": "var(--wp--preset--color--chocolate-brown)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--cappuccino-brown)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--cappuccino-brown)"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "1.25rem",
			"padding": {
				"top": "var(--wp--custom--spacing--small)",
				"right": "var(--wp--custom--spacing--small)",
				"bottom": "var(--wp--custom--spacing--small)",
				"left": "var(--wp--custom--spacing--small)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--poppins)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontWeight": "400",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
